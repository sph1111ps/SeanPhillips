generator client {
  provider   = "prisma-client-js"
  engineType = "node-api"
}
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Project {
  id        Int      @id @default(autoincrement())
  slug      String   @unique
  title     String
  summary   String
  content   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}